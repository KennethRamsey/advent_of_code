<Query Kind="Program">
  <AutoDumpHeading>true</AutoDumpHeading>
</Query>


void Main()
{
    part1();
    part2();
}

void part1()
{
    var lines = data3
                .Split(Environment.NewLine)
                .ToArray()
                //.Dump()
                ;

    var width = lines[0].Length;
    var height = lines.Length;

    var vertLines = getVertLines().ToArray();
    //vertLines.Dump();

    var diagToRightLines = getDiagLines(lines).ToArray();
    //diagToRightLines.Dump();

    var reverseLines = lines.Select(s => string.Join("", s.Reverse())).ToArray();
    var diagToLeftLines = getDiagLines(reverseLines).ToArray();
    //diagToLeftLines.Dump();


    var allLines = new[]
    {
        lines,
        vertLines,
        diagToRightLines,
        diagToLeftLines
    };

    allLines
    .Select(ls =>
        ls
        .Select(l => Regex.Count(l, "XMAS") + Regex.Count(l, "SAMX"))
        .Sum()
    )
    .Sum()
    .Dump();

    //////////////

    IEnumerable<string> getVertLines()
    {
        var sb = new StringBuilder();
        for (int x = 0; x < width; x++)
        {
            for (int y = 0; y < height; y++)
            {
                sb.Append(lines[y][x]);
            }
            yield return sb.ToString();
            sb.Clear();
        }
    }

    IEnumerable<string> getDiagLines(string[] lines)
    {
        var diags = width + height - 1;
        var sb = new StringBuilder();
        for (int d = 0; d < diags; d++)
        {
            for (int delta = d; delta >= 0; delta--)
            {
                var (x, y) = (delta, d - delta);
                var exist = 0 <= x && x < width
                            && 0 <= y && y < height;
                if (exist)
                {
                    sb.Append(lines[y][x]);
                }
            }

            yield return sb.ToString();
            sb.Clear();
        }
    }
}


void part2()
{
    var lines = data3
                .Split(Environment.NewLine)
                .ToArray();

    var width = lines[0].Length;
    var height = lines.Length;

    var vertLines = getVertLines().ToArray();

    var reverseLines = lines.Select(s => string.Join("", s.Reverse())).ToArray();
    var reverseVertLines = vertLines.Select(s => string.Join("", s.Reverse())).ToArray();


    var allOrientations = new[]
    {
        lines,
        reverseLines,
        vertLines,
        reverseVertLines
    };

    allOrientations
    .Select(X_MAS_Count)
    .Sum()
    .Dump();

    //////////////

    IEnumerable<string> getVertLines()
    {
        var sb = new StringBuilder();
        for (int x = 0; x < width; x++)
        {
            for (int y = 0; y < height; y++)
            {
                sb.Append(lines[y][x]);
            }
            yield return sb.ToString();
            sb.Clear();
        }
    }

    int X_MAS_Count(string[] lines)
    {
        var count = 0;
        var xmasPoints = new[]
        {
            (0, 0, 'M'), // (x,y,match)
            (0, 2, 'M'),
            (1, 1, 'A'),
            (2, 0, 'S'),
            (2, 2, 'S')
        };

        for (int y = 0; y < height - 2; y++)
        {
            for (int x = 0; x < width - 2; x++)
            {
                var good = xmasPoints.All(p => lines[y + p.Item2][x + p.Item1] == p.Item3);
                if (good)
                    count++;
            }
        }

        return count;
    }
}



